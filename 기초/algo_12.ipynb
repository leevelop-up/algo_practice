    "import copy\n",
    "N, A = int(input()), list(map(int, input().split()))\n",
    "DP = copy.deepcopy(A)\n",
    "for i in range(N):\n",
    "    for j in range(i):\n",
    "        if A[i] > A[j] and DP[i] <A[i]+DP[j]:\n",
    "            DP[i] = A[i]+DP[j]\n",
    "\n",
    "print(max(DP))\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n"
   ]
