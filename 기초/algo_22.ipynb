    "N = int(input())\n",
    "M = list(map(int,input().split()))\n",
    "\n",
    "repack = 0\n",
    "for i in range(N):\n",
    "    repack += M[i]/max(M)*100;\n",
    "    \n",
    "print(repack/N)\n",
    "\n",
